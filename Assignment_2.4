        "def extract_ne(quote):\n",
        "  words = word_tokenize(quote)\n",
        "  tags = nltk.pos_tag(words)\n",
        "  tree = nltk.ne_chunk(tags, binary=True)\n",
        "  return set(\" \".join(i[0] for i in t)\n",
        "            for t in tree\n",
        "            if hasattr(t, \"label\") and t.label() == \"NE\"\n",
        "            )"
